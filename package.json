{
  "name": "remix-template-vercel-ts",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "generate:css": "tailwindcss -i ./app/styles/tailwind.css -o ./app/tailwind.css",
    "build:css": "yarn generate:css -- --minify",
    "build:remix": "remix build",
    "build": "yarn run-s build:*",
    "dev:css": "yarn generate:css -- --watch",
    "dev:remix": "remix dev",
    "dev": "dotenv -e .env yarn run-p dev:*",
    "test": "dotenv -e .env yarn run vitest",
    "postinstall": "remix setup node"
  },
  "dependencies": {
    "@remix-run/react": "^1.3.3",
    "@remix-run/vercel": "^1.3.3",
    "@supabase/supabase-js": "^1.32.2",
    "clsx": "^1.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.3.3",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.3.3",
    "@remix-run/eslint-config": "^1.3.3",
    "@remix-run/serve": "^1.3.3",
    "@tailwindcss/forms": "^0.5.0",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^14.0.0",
    "@types/eslint": "^8.4.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@vitejs/plugin-react": "^1.2.0",
    "autoprefixer": "^10.4.4",
    "c8": "^7.11.0",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.11.0",
    "happy-dom": "^2.51.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.5",
    "vite-tsconfig-paths": "^3.4.1",
    "vitest": "^0.8.0"
  },
  "eslintIgnore": [
    "node_modules",
    ".cache",
    ".env",
    ".vercel",
    ".output",
    "/build/",
    "/public/build",
    "/api/index.js",
    "/app/tailwind.css"
  ],
  "engines": {
    "node": ">=14"
  }
}
